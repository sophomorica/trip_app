<%= form_with(model:[@location, @address], local:true, class:"ui form") %>
  <div class='field'>
    <%= f.label :street %>
    <%= f.text_field :street, autofocus:true, required:false, placeholder:"123456 Yucca Needle Ave"%>
  </div>
    <%= f.label :city %>
    <%= f.text_field :city, required:false, placeholder:"Saguaro City"%>
  </div>
    <%= f.label :state %>
    <%= f.text_field :state, required:false, placeholder:"Ut"%>
  </div>
    <%= f.label :lat %>
    <%= f.text_field :lat, required:false, placeholder:"38.611128"%>
  </div>
    <%= f.label :long %>
    <%= f.text_field :long, required:false, placeholder:"-109.608368"%>

  <%= f.submit "Save" %>
<% end %>
